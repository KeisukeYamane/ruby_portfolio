<h2>ユーザー一覧</h2>
<div class="container">
  <div class="row">
    <% @user.each do |user|%>
    <div class="col-lg-4 col-sm-6">
      <% unless user.name == '管理者' %>
      <div class="outer-profile">
        <div class="profile">
          <div class="d-flex align-items-center">
            <div class=""><%= link_to image_tag("/user_image/#{user.image}", :size =>'90x90'), user_path(user.id)%></div>
          </div>
          <div clss=""><p><%= link_to user.name, user_path(user.id) %></p></div>
        </div>
      </div>
    </div>
    <% end %>
    <% end %>
  </div>
</div>