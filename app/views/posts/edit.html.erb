<div class="">
  <div class="">
    <h1>投稿編集</h1>
    <%= form_for(@post, html: {multipart: true}) do |f| %>
    <div class="">
      <div class="">
        <% @post.errors.full_messages.each do |message| %>
        <div class=""><%= message %></div>
        <% end %>
        <%= f.text_field :title %>
        <%= f.text_area :content %>
        <%= file_field_tag :upfile %>
        <%= f.submit '投稿する' %>
      </div>
    </div>
    <% end %>
  </div>
</div>
