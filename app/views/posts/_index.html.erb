<% @posts.each do |post|%>
<div class="">
  <div class="">
    <img src="<%= "/user_image/#{post.user.image}"%>">
  </div>
  <div class="">
    <div class="">
      <%= link_to post.user.name, "/users/#{post.user.id}"%>
    </div>
    <%= link_to post.title, post_path(post.id) %>
  </div>
</div>
<% end %>
  <% @pagenation.each do |key, value| %>
    <%= link_to key, posts_page_index_path(value), remote: true %>
  <% end %>

